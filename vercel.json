{
  "version": 2,
  "builds": [
    { "src": "api/*.py", "use": "@vercel/python" }
  ],
  "routes": [
    { "src": "/api/stt", "dest": "api/stt.py" },
    { "src": "/api/orchestrate", "dest": "api/orchestrate.py" },
    { "src": "/api/tts", "dest": "api/tts.py" },
    { "src": "/api/mcps", "dest": "api/mcps.py" }
  ]
} 